{
  "name": "core-nestjs",
  "version": "1.0.0",
  "description": "",
  "author": "Dinesh Kumar",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "nest build && node dist/main",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "commit": "git-cz",
    "docs": "typedoc --options typedoc.json",
    "seed": "sequelize-cli db:seed --seed",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:fresh": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all"
  },
  "dependencies": {
    "@nestjs/common": "^8.3.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.3.1",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.3.1",
    "@nestjs/platform-socket.io": "^8.3.1",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/testing": "^8.3.1",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/websockets": "^8.3.1",
    "bcrypt": "5.0.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.0",
    "express": "4.17.1",
    "helmet": "^5.0.2",
    "husky": "7.0.1",
    "ioredis": "^4.27.7",
    "jsonapi-serializer": "3.6.7",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "moment": "^2.29.1",
    "nestjs-redis": "git+https://github.com/GyanendroKh/nestjs-redis.git#nest8-fix",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "sequelize": "^6.16.1",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.3",
    "slugify": "^1.6.5",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "8.0.2",
    "@types/bcrypt": "3.0.0",
    "@types/cron": "^1.7.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/jsonapi-serializer": "3.6.4",
    "@types/lodash": "4.14.168",
    "@types/node": "16.6.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.11",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.0",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "supertest": "6.1.5",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typedoc": "^0.21.5",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@shared/*": "./dist/shared/*",
    "@constants/*": "./dist/constants/*",
    "@filters/*": "./dist/filters/*",
    "@guards/*": "./dist/guards/*",
    "@interfaces/*": "./dist/interfaces/*",
    "@pipes/*": "./dist/pipes/*",
    "@interceptors/*": "./dist/interceptors/*",
    "@decorators/*": "./dist/decorators/*",
    "@exceptions/*": "./dist/exceptions/*",
    "@utils/*": "./dist/utils/*",
    "@modules/*": "./dist/modules/*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
