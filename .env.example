SENDGRID_API_KEY=

# Mail configruration
MAIL_HOST=0.0.0.0
MAIL_PORT=1025
MAIL_USER=null
MAIL_PASS=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="no-reply@goodaction.com"
MAIL_FROM_NAME=

# JWT configuration
JWT_SECRET=
JWT_DURATION=1200

# Twilio
TWILIO_ENABLED=true
TWILIO_FROM=
TWILIO_ACCOUNT_SID=
