# Environment
NODE_ENV=development
NEST_DEBUG=false
DEBUG=false

SERVER_NAME=
SERVER_SCHEMA=http
SERVER_HOST="localhost"
SERVER_PORT=3000
SERVER_DEBUG_PORT=9229
SERVER_WEB_SOCKER_PORT=3002
SERVER_TZ=America/Los_Angeles

# Postgre database configuration
DB_DIALECT=postgres
DB_HOST=localhost
DB_PORT=5432
